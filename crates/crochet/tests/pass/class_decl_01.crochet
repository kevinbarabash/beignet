class Foo {
    msg: string;
    static tag = "Foo";

    constructor(self) {}

    bar(self) { "hello"; }
    static add(x, y) { x + y; }
    get baz(self) { "world!"; }
    set qux(mut self, msg: string) {}
}

let f1: Foo = new Foo();
let msg1 = f1.bar();

let f2: mut Foo = new Foo();
f2.qux = "goodbye";
let msg2 = f2.bar();
